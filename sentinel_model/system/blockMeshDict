xl -0.18;
xu 0.18;
yl -0.15;
yu 0.28;
zl -0.163;
zu 0.15;

vertices
(
    ( $xl  $yl  $zl)
    ($xu  $yl  $zl)
    ($xu  $yu  $zl)
    ( $xl  $yu  $zl)
    ( $xl  $yl  $zu)
    ($xu  $yl  $zu)
    ($xu  $yu  $zu)
    ( $xl  $yu  $zu)
);

blocks
(
    hex (0 1 2 3 4 5 6 7) (45 54 39) 
    simpleGrading 
    (1 1 1)
);

edges
(
);

boundary
(
    left
    {
        type patch;
        faces
        (
            (0 4 7 3)
        );
    }
    right
    {
        type patch;
        faces
        (
            (2 6 5 1)
        );
    }
    up
    {
        type wall;
        faces
        (
            (4 5 6 7)
        );
    }

    down
    {
        type wall;
        faces
        (
            (0 3 2 1)
        );
    }
    inlet
    {
        type wall;
        faces
        (
            (0 1 5 4)
        );
    }
    outlet
    {
        type wall;
        faces
        (
            (3 7 6 2)
        );
    }
);

mergePatchPairs
(
);

