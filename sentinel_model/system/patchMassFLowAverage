pressure1
{
    // Mandatory entries (unmodifiable)
    type            pressure;
    libs            ("libfieldFunctionObjects.so");

    // Mandatory entries (runtime modifiable)
    mode            total;

    // Optional entries (runtime modifiable)
    p               p;
    U               U;
    rho             rhoInf;
    rhoInf          1.0; // enabled if rho=rhoInf
    pRef            0.0; //101325.0;
    hydroStaticMode none;
    g               (0 -9.81 0); // enabled if hydroStaticMode != none
    hRef            0.0;         // enabled if hydroStaticMode != none
    pInf            0.0;
    UInf            (1 0 0);
}

Mach_inlet
{
    type            surfaceFieldValue;
    libs            ("libfieldFunctionObjects.so");

    log             true;
    writeControl    writeTime;
    writeFields     true;

    surfaceFormat   none;
    regionType      patch;
    name            inlet;

    operation       areaNormalAverage; //weightedAreaAverage;
    //weightField     phi;
    scaleFactor     -0.0029412;
    fields          (U);
}

pt_inlet
{
    type            surfaceFieldValue;
    libs            ("libfieldFunctionObjects.so");

    log             true;
    writeControl    writeTime;
    writeFields     true;

    surfaceFormat   none;
    regionType      patch;
    name            inlet;

    operation       weightedAreaAverage;
    weightField     phi;
    scaleFactor     1.225;
    fields          (total(p));
}

